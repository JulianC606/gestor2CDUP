<p id="notice"><%= notice %></p>

<h1>Accounting Entries</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Transaction</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @accounting_entries.each do |accounting_entry| %>
      <tr>
        <td><%= accounting_entry.date %></td>
        <td><%= accounting_entry.transaction_id %></td>
        <td><%= link_to 'Show', accounting_entry %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

